<div class="container">
  <div class="row">
    <div class="col s12 center-aligned">
      <h2>Take a Quiz</h2>
    </div>
  </div>
    <div class="divider"></div>
  <div class="row">
    <div class="col s9">
      <div class="section">
        <h5>{{Quizzes.title}}</h5>
        <p>{{User.email}}</p>
      </div>
    </div>
  </div>
  <div class="divider"></div>
   {{#each Quizzes}}
  <div class="divider"></div>
  <div class="row">
    <div class="col s9">
      <div class="section">
        <h5>{{dataValues.title}}</h5>
      </div>
    </div>
    <div class="col s3">
      <a href="./currentquiz" class="waves-effect waves-light btn-large take-quiz">Take Quiz</a>
    </div>
  </div>
  <div class="divider"></div>
  {{/each}}
</div>